snippet d
	debugger
snippet f
	function(${1}) {
		${3}
	}${2}
snippet fu
	function ${1}(${2}) {
		${3}
	}
snippet m
	${1}: function(${2}) {
		${3}
	},
snippet clos
	(function() {
		${1}
	})();
snippet r
	return
snippet re
