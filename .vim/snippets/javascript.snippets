		onSetMod: {

			js: {
				inited: function() {
					${1}
				}
			}

		}

	});
snippet should
	({
		shouldDeps: ${1}
	})
snippet logger
	logger = $('<ul>').css({
		'position': 'absolute',
		'top': 0,
		'right': 0,
		'z-index': 9999,
		'background-color': '#fff'
	}).appendTo($('body'))
	function log(s) {
		logger.append('<li>' + s + '</li>')
	};
snippet pcss
