		"name": "${1}",
		"version": "${2:0.0.1}",
		"repository": {
			"type": "git",
			"url": "git://github.yandex-team.ru/${3}.git"
		},
		"engines": {
			"node": "0.8 | 0.9 | 0.10"
		},
		"dependencies": {
			${4}
		},
		"devDependencies": {},
		"private": true
	}
snippet tech
	require('enb/techs/${1}')${2}
snippet techs
	[require('enb/techs/${1}'), { ${2} }]${3}
snippet xhr
	var xhr = new XMLHttpRequest();
	xhr.open('GET', '${1}', true);
	xhr.onreadystatechange = function() {
		if (xhr.readyState == 4)
			console.log(xhr);
		callback(xhr.responseText, xhr.status);
	}
	xhr.send();
